{
  "name": "calc-react",
  "version": "0.0.1",
  "description": "A simple calculator written in React.js w/Redux",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:debug": "node debug --harmony ./node_modules/.bin/jest --runInBand",
    "build": "browserify index.js -t babelify --debug --outfile bundle.js",
    "build:dist": "browserify index.js -t babelify | uglifyjs - | bundle-collapser - > bundle.min.js",
    "watch": "watchify -v --debug -t babelify index.js -o bundle.js",
    "clean": "rm bundle.js"
  },
  "keywords": [
    "calc",
    "react.js",
    "redux"
  ],
  "author": "Daniel Mikusa <dan@mikusa.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "immutable": "^3.8.1",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-jest": "^12.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "bundle-collapser": "^1.2.1",
    "jest-cli": "^12.1.1",
    "react-addons-test-utils": "^15.1.0",
    "supervisor": "^0.10.0",
    "uglify-js": "^2.7.3",
    "watchify": "^3.7.0"
  }
}
