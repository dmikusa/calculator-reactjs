    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-jest": "^12.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "jest-cli": "^12.1.1",
    "react-addons-test-utils": "^15.1.0",
    "supervisor": "^0.10.0",
